<div class="admin-title">
    <h2>Posts</h2>
    <a href="/add-post" class="button">Add New</a>
</div>

<ul class="admin-post">

    <% data.forEach(post=> { %>
        <li>

            <a href="/posts/<%= post._id %>">

                <%= post.title %> &nearr;
            </a>

        </li>
        <% }) %>

</ul>